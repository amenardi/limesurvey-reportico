FROM {lime_}questions AS q
LEFT JOIN {lime_}groups AS g ON q.sid = g.sid
AND q.gid = g.gid
AND q.language = g.language
LEFT JOIN {lime_}questions AS sq ON q.qid = sq.parent_qid
AND q.language = sq.language
WHERE q.parent_qid =0
[ AND g.gid IN ({group}) ]
[ AND g.sid IN ({survey}) ]
ORDER BY g.group_order, q.question_order, sq.scale_id DESC , sq.question_order
